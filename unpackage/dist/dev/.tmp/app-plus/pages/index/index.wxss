.my-device {
  display: inline-block;
  width: 46%;
  margin: 20rpx 1% 0 2%;
  border-radius: 8px;
  border: 1px solid #333;
}
.device-top {
  padding: 15rpx 0;
  border-bottom: 1px solid #333;
}
.device-main {
  padding: 20rpx;
  box-sizing: border-box;
}
.device-main-left {
  width: 45%;
}
.device-main-right {
  width: 55%;
}
.device-main-right {
  line-height: 1.5;
}
.device-main-left,
.device-main-right {
  display: inline-block;
  vertical-align: middle;
}
.device-main-left image {
  width: 60rpx;
  height: 60rpx;
}
.device-container {
  padding-bottom: 30rpx;
}
.device-bottom {
  color: #2794F0;
  text-align: center;
  padding: 15rpx 0;
  border-top: 1px solid #333;
  font-size: 30rpx;
}
.status.online {
  border: 1px solid #6BBD8F !important;
  color: #6BBD8F !important;
}
.status.offline {
  border: 1px solid #999999;
  color: #999999;
  margin-left: 20rpx;
}
.float-btn {
  position: fixed;
  bottom: 150rpx;
  right: 50rpx;
  width: 100rpx;
  height: 100rpx;
