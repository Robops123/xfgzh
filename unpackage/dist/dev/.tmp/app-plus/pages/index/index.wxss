.top-entrance {
  position: absolute;
  height: 96px;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
}
.bg-wrapper {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #040B29;
  opacity: 0.89;
  box-shadow: 0px 2px 4px #b1b4bf;
}
.bg-content {
  position: relative;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
