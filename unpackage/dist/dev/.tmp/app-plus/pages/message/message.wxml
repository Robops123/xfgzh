<view class="purchase-list"><my-tabs vue-id="1" modelData="{{modelData}}" badges="{{badges}}" initIndex="{{initIndex}}" data-event-opts="{{[['^change',[['tapChange']]]]}}" bind:change="__e" bind:__l="__l"></my-tabs><scroll-view class="purchase-body" style="height:calc(100vh - 260rpx);" scroll-y="true" data-event-opts="{{[['scrolltolower',[['scrolltolower',['$event']]]],['scrolltoupper',[['scrolltoupper',['$event']]]],['touchstart',[['touchstart',['$event']]]],['touchend',[['touchend',['$event']]]]]}}" bindscrolltolower="__e" bindscrolltoupper="__e" bindtouchstart="__e" bindtouchend="__e"><block wx:for="{{userList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['list',(pickerUserIndex==index)?'active':'']}}" data-index="{{index}}"><block wx:if="{{item.status==0}}"><image class="status" src="../../static/img/message/cl.png" mode></image></block><block wx:if="{{item.status==2}}"><image class="status" src="../../static/img/message/clz.png" mode></image></block><block wx:if="{{item.status==3}}"><image class="status" src="../../static/img/message/wb.png" mode></image></block><view class="title">{{''+item.title+''}}</view><view class="brief"><view><image src="../../static/img/device/location.png" mode></image></view><view><view class="address cblue">{{item.devLocation}}</view><view class="date coffline">{{item.updateTime}}</view></view></view><block wx:if="{{item.status==1}}"><view class="operate"><block wx:if="{{curType==0}}"><view data-event-opts="{{[['tap',[['changeStatus',['$0',0,index],[[['userList','',index,'id']]]]]]]}}" class="cblue" bindtap="__e">解除</view></block><view data-event-opts="{{[['tap',[['changeStatus',['$0',3,index],[[['userList','',index,'id']]]]]]]}}" class="cblue" bindtap="__e">误报</view><block wx:if="{{curType==1}}"><view data-event-opts="{{[['tap',[['toFix',['$0'],[[['userList','',index]]]]]]]}}" class="cblue" bindtap="__e">报修</view></block></view></block></view></block><view data-event-opts="{{[['tap',[['hidePop',['$event']]]]]}}" hidden="{{!(showShade)}}" class="shade" bindtap="__e"><view class="{{['pop',(showPop)?'show':'']}}" style="{{(popStyle)}}"><block wx:for="{{popButton}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-index="{{index}}" data-event-opts="{{[['tap',[['pickerMenu',['$event']]]]]}}" bindtap="__e">{{item}}</view></block></view></view></scroll-view><min-action-sheet class="vue-ref" vue-id="2" data-ref="as" bind:__l="__l"></min-action-sheet><my-loading vue-id="3" bind:__l="__l"></my-loading></view>