page {
  background: #F0F0F0;
}
.yt-list {
  margin-top: 16rpx;
  background: #fff;
}
.yt-list-cell {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  line-height: 70rpx;
  position: relative;
}
.yt-list-cell.cell-hover {
    background: #fafafa;
}
.yt-list-cell.b-b:after {
    left: 30rpx;
}
.yt-list-cell .cell-icon {
    height: 32rpx;
    width: 32rpx;
    font-size: 22rpx;
    color: #fff;
    text-align: center;
    line-height: 32rpx;
    background: #f85e52;
    border-radius: 4rpx;
    margin-right: 12rpx;
}
.yt-list-cell .cell-icon.hb {
      background: #ffaa0e;
}
.yt-list-cell .cell-icon.lpk {
      background: #3ab54a;
}
.yt-list-cell .cell-tit {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    font-size: 26rpx;
    color: dimgray;
    margin-right: 10rpx;
}
.yt-list-cell .cell-tip {
    font-size: 26rpx;
    color: #f85e52;
}
.yt-list-cell .cell-tip.disabled {
      color: #f85e52;
}
.yt-list-cell .cell-tip.active {
      color: #f85e52;
}
.yt-list-cell .cell-tip.red {
      color: #f85e52;
}
.yt-list-cell.desc-cell {
    position: relative;
}
.yt-list-cell.desc-cell .cell-tit {
      max-width: 160rpx;
}
.yt-list-cell .desc {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    font-size: 28rpx;
    color: darkgray;
}
.yt-list-cell switch {
    -webkit-transform: translateX(16rpx) scale(0.84);
            transform: translateX(16rpx) scale(0.84);
}
.add-btn {
  position: fixed;
  left: 30rpx;
  right: 30rpx;
  bottom: 16rpx;
  z-index: 95;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  width: 690rpx;
  height: 80rpx;
  font-size: 32rpx;
  color: #fff;
  background-color: #fa436a;
  border-radius: 10rpx;
  box-shadow: 1px 2px 5px rgba(219, 63, 96, 0.4);
}
.mini-btn {
  line-height: 32px;
  height: 32px;
  margin-right: 10rpx;
}
.map-warpper {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.card {
  padding: 20rpx 20rpx 0;
  background-color: #fff;
  margin: 30rpx;
  border-radius: 8px;
}
.card .line {
  padding: 10rpx 0;
}
.card .border-line {
  border-bottom: 1px solid #f2f2f2;
}
.edit-btn {
  padding: 20rpx;
  text-align: center;
  border-top: 1px solid #E6E6E6;
}
.add-share-btn {
  padding: 5rpx 20rpx;
  background: #2a95f0;
  border-radius: 24px;
  color: #fff;
  margin-top: -5rpx;
}
.card2 {
  padding: 0 !important;
  margin: 40rpx 30rpx;
}
.card2 .border-line {
  font-size: 24rpx;
}
.card2 text {
  display: inline-block;
  padding: 0 10rpx;
  line-height: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
}
.card2 .left-border {
  border-left: 1px solid #111;
}
.co1 {
  width: 16%;
}
.col2 {
  width: 34%;
}
.col3 {
  width: 29%;
}
.col4 {
  width: 20%;
}
.sharemap {
  height: 800rpx !important;
}
.btn {
  background: #3f87ff !important;
  border-radius: 34px !important;
  font-size: 34rpx;
  margin-top: 60rpx;
}
.input {
  margin: 20rpx 0;
  padding: 15rpx 0;
  font-size: 28rpx;
  width: 85%;
  border: 1px solid #f2f2f2;
}

